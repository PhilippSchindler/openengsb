<?xml version="1.0"?>
<rdf:RDF
    xmlns="http://www.cdl.ifs.tuwien.ac.at/pm.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.cdl.ifs.tuwien.ac.at/pm.owl">
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="Notification"/>
  <owl:Class rdf:ID="Project"/>
  <owl:Class rdf:ID="Engineer">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Person"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Test_Case"/>
  <owl:Class rdf:ID="Client">
    <rdfs:subClassOf rdf:resource="#Person"/>
  </owl:Class>
  <owl:Class rdf:ID="Test_Run"/>
  <owl:Class rdf:ID="Requirement"/>
  <owl:ObjectProperty rdf:ID="hasProjectLeader">
    <rdfs:domain rdf:resource="#Project"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="hasParticipant"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasRecipient">
    <rdfs:domain rdf:resource="#Notification"/>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isCreatedBy">
    <rdfs:range rdf:resource="#Person"/>
    <rdfs:domain rdf:resource="#Requirement"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isProjectLeaderOf">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="isParticipantOf"/>
    </rdfs:subPropertyOf>
    <rdfs:domain rdf:resource="#Engineer"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isParticipantOf">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Project"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasTestRequirements">
    <rdfs:range rdf:resource="#Requirement"/>
    <rdfs:domain rdf:resource="#Test_Case"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasExecutor">
    <rdfs:domain rdf:resource="#Test_Run"/>
    <rdfs:range rdf:resource="#Engineer"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasProjectOwner">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#hasParticipant"/>
    </rdfs:subPropertyOf>
    <rdfs:domain rdf:resource="#Project"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isProjectOwnerOf">
    <rdfs:subPropertyOf rdf:resource="#isParticipantOf"/>
    <rdfs:domain rdf:resource="#Client"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasParticipant">
    <rdfs:range rdf:resource="#Person"/>
    <rdfs:domain rdf:resource="#Project"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasTestCase">
    <rdfs:domain rdf:resource="#Test_Run"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="#Test_Case"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isPartOfProject">
    <rdfs:range rdf:resource="#Project"/>
    <rdfs:domain rdf:resource="#Requirement"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="hasMessage">
    <rdfs:domain rdf:resource="#Notification"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasEmail">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Person"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasEndDate">
    <rdfs:domain rdf:resource="#Project"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasPhase">
    <rdfs:domain rdf:resource="#Project"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >initial</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">
                <rdf:rest rdf:parseType="Resource">
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >closing</rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:rest>
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >testing</rdf:first>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >production</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >requirement</rdf:first>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasRunTime">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#Test_Run"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasTestStatus">
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >tested_not_ok</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >initial</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >in_progress</rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:rest>
            </rdf:rest>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >tested_ok</rdf:first>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#Test_Run"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasTestCaseText">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Test_Case"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasPriority">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >high</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >low</rdf:first>
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >medium</rdf:first>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdfs:domain rdf:resource="#Notification"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasRequirementText">
    <rdfs:domain rdf:resource="#Requirement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="name">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Requirement"/>
          <owl:Class rdf:about="#Project"/>
          <owl:Class rdf:about="#Test_Case"/>
          <owl:Class rdf:about="#Notification"/>
          <owl:Class rdf:about="#Person"/>
          <owl:Class rdf:about="#Test_Run"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="isCritical">
    <rdfs:domain rdf:resource="#Requirement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasStartDate">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:domain rdf:resource="#Project"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasRequirementStatus">
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >initial</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >in_progress</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >ready_for_test</rdf:first>
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:rest>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#Requirement"/>
  </owl:DatatypeProperty>
</rdf:RDF>

